<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/b660d10224389a5df8e7d51557d7aeca"

    >


    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_gravity="center"
        android:layout_marginTop="15dp"
        android:background="@android:color/transparent"
        android:onClick="irTelaPreparo"
        android:scaleType="fitStart"
        app:srcCompat="@drawable/make___copia" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="180dp"
        android:layout_height="177dp"
        android:layout_gravity="center"
        android:layout_marginTop="14dp"
        android:background="@android:color/transparent"
        android:onClick="irTelaCriarAgendamento"
        android:scaleType="fitStart"
        app:srcCompat="@drawable/agendar___copia" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="180dp"
        android:layout_height="183dp"
        android:layout_gravity="center"
        android:layout_marginTop="14dp"
        android:background="@android:color/transparent"
        android:scaleType="fitStart"
        app:srcCompat="@drawable/agenda___copia" />


</LinearLayout>